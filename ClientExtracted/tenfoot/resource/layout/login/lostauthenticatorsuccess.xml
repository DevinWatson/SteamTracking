<root>
	<styles>
		<include src="file://{resources}/styles/steamstyles.css" />
		<include src="file://{resources}/styles/wizardstyles.css" />
	</styles>

	<ChangingPasswordWizardChild class="WizardChild" tabindex="auto" selectionpos="auto">
		<Label class="WizardChildTitle" text="#LostAuthenticator_Title" />
		<Label class="WizardChildDescription" text="#LostAuthenticator_Success_Description" />

		<Panel id="WizardButtons" tabindex="auto" selectionpos="auto">
			<Button class="WizardButton" id="ButtonDone" onactivate="LostAuthenticatorDone()">
				<Label id="Label" text="#WizardButton_Done" />
			</Button>
		</Panel>

	</ChangingPasswordWizardChild>

</root>
